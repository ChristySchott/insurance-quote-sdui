  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@insurance-quote/shared": "workspace:*",
    "fastify": "^4.25.2"
  },
  "devDependencies": {
    "@insurance-quote/eslint-config": "workspace:*",
    "@insurance-quote/prettier-config": "workspace:*",
    "@types/node": "^20.10.6",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "pino-pretty": "13.1.2"
  }
}
